{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#3676d6",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "style": "diamond",
          "template": " {{ .Name }} ",
          "trailing_diamond": "\ue0b0",
          "type": "shell"
        },
        {
          "background": "#ffb847",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "diamond",
          "template": " \uea83  {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#fffb38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#193549",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "overflow": "hide",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "trailing_diamond": "\ue0b0",
          "type": "node"
        },
        {
          "background": "#8ED1F7",
          "foreground": "#111111",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "julia"
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "style": "diamond",
          "overflow": "hide",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "python"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "diamond",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "diamond",
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "display_default": false
          },
          "style": "diamond",
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "aws"
        },
        {
          "background": "#ffff66",
          "foreground": "#111111",
          "leading_diamond": "<transparent>\ue0b0</>",
          "style": "diamond",
          "template": " \uf0ad ",
          "trailing_diamond": "\ue0b0",
          "type": "root"
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#262626",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "style": "diamond",
          "overflow": "hide",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "trailing_diamond": "\ue0b0",
          "type": "battery"
        },
        {
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "leading_diamond": "<transparent>\ue0b0</>",
          "properties": {
            "time_format": "_2-Jan,15:04"
          },
          "style": "diamond",
          "overflow": "hide",
          "template": "{{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "leading_diamond": "<transparent>\ue0b0</>",
          "style": "diamond",
          "overflow": "hide",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        },
        {
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "overflow": "hide",
          "leading_diamond": "<transparent>\ue0b0</>",
          "template": " \ue23a ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#0fb92e",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "enable_cursor_positioning": true,
  "version": 3
}
